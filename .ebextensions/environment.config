option_settings:
  aws:elasticbeanstalk:application:environment:
    # App configuration
    APP_PORT: 8080
    APP_NAME: "OPN Commerce Backend"
    NODE_ENV: production
    
    # CORS configuration
    CORS_ENABLED: "true"
    CORS_ORIGINS: "*"
    
    # API Documentation
    API_DOCS_ENABLED: "true"
    API_DOCS_PATH: "api"
    
    # Database (configure as needed)
    DB_TYPE: "postgres"
    # Add your database credentials via EB environment variables
    
